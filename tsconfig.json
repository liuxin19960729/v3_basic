{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    // 模块代码
    "xxengine/*.ts",
    "xxengine/*.d.ts"
  ],
  "compilerOptions": {
    "strictNullChecks": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "__XXENGINE__/*":["xxengine/*"]
    },
    "lib": ["ES2017","DOM"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
  ]
}